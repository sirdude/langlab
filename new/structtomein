#!/usr/bin/mein
# This program reads in an AST of simple chars and converts it to tokens in our specified 
# language.

string infile, outfile;

use AST;
use prompt;

infile = ARGV[0];
outfile = ARGV[1];

if (!infile) {
	infile  = prompt("Infile: ");
}
if (!outfile) {
	outfile = prompt("Outfile: ");
}

object ast = load_ast(infile);
write_ast(outfile, ast);
